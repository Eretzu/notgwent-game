{
	"name": "server",
	"version": "0.0.0",
	"license": "GNU GPL v3.0",
	"private": true,
	"scripts": {
		"build": "tsc && mkdir -p dist/server/src/views && cp src/views/*.jade dist/server/src/views",
		"start": "node dist/server/src/app.js",
		"serve": "npx nodemon --legacy-watch --watch 'src' --watch '../shared/src' -e 'ts,jade' --exec 'npm run serve-exec'",
		"serve-exec": "npx tsc --incremental --tsBuildInfoFile dist/tsBuildInfo.json && mkdir -p dist/server/src/views && cp src/views/*.jade dist/server/src/views && node dist/server/src/app.js",
		"watch": "babel --watch src --out-dir dist --extensions .ts,.tsx"
	},
	"dependencies": {
		"canvas": "^2.6.1",
		"@babel/runtime": "^7.6.3",
		"babel-node": "0.0.1-security",
		"babel-polyfill": "^6.26.0",
		"bcrypt": "^5.0.0",
		"cookie-parser": "~1.4.4",
		"cors": "^2.8.5",
		"db-migrate": "^0.11.10",
		"debug": "~2.6.9",
		"express": "~4.16.1",
		"express-ws": "^4.0.0",
		"glob": "^7.1.6",
		"http-errors": "~1.6.3",
		"jade": "~1.11.0",
		"jsonwebtoken": "^8.5.1",
		"module-alias": "^2.2.2",
		"morgan": "~1.9.1",
		"njstrace": "^1.2.0",
		"node-pg-migrate": "^3.23.3",
		"node-uuid": "^1.4.8",
		"pg": "^7.12.1",
		"rgb-hex": "^3.0.0",
		"seedrandom": "^3.0.5",
		"sequelize": "^5.19.6",
		"tspath": "^1.3.7",
		"uuid": "^3.3.3"
	},
	"devDependencies": {
		"@babel/cli": "^7.6.4",
		"@babel/core": "^7.6.4",
		"@babel/plugin-proposal-class-properties": "^7.5.5",
		"@babel/plugin-proposal-object-rest-spread": "^7.6.2",
		"@babel/plugin-transform-runtime": "^7.6.2",
		"@babel/preset-env": "^7.6.3",
		"@babel/preset-flow": "^7.0.0",
		"@babel/preset-typescript": "^7.6.0",
		"@types/express": "^4.17.1",
		"@types/express-session": "^1.15.14",
		"@types/express-ws": "^3.0.0",
		"@types/jsonwebtoken": "^8.3.4",
		"@types/pg": "^7.11.2",
		"@types/sequelize": "^4.28.6",
		"@types/uuid": "^3.4.5",
		"@typescript-eslint/eslint-plugin": "^3.8.0",
		"@typescript-eslint/parser": "^3.8.0",
		"acorn": "^7.1.1",
		"babel-eslint": "^10.0.3",
		"babel-plugin-module-resolver": "^4.0.0",
		"babel-preset-es2015": "^6.24.1",
		"babel-watch": "^7.0.0",
		"copyfiles": "^2.3.0",
		"eslint": "^6.5.1",
		"eslint-plugin-flowtype": "^4.3.0",
		"eslint-plugin-node": "^10.0.0",
		"express-session": "^1.17.0",
		"flow-bin": "^0.109.0",
		"nodemon": "^2.0.3",
		"pgmigrate": "^5.0.0",
		"ts-node": "^8.6.2",
		"typescript": "^3.6.4"
	},
	"_moduleAliases": {
		"@shared": "dist/shared/src"
	},
	"babel": {
		"presets": [
			"@babel/preset-env",
			"@babel/preset-typescript"
		],
		"plugins": [
			"@babel/proposal-class-properties",
			"@babel/proposal-object-rest-spread",
			"@babel/plugin-transform-runtime",
			[
				"module-resolver",
				{
					"alias": {
						"@shared": "../shared/src"
					}
				}
			]
		]
	}
}
